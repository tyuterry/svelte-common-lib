{
	"name": "editor-lib",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "npm run build-rollup && npm run build-rm-dts && npm run  build-rollup-dts",
		"build-rollup": "rollup -c rollup.config.js",
		"build-rm-dts": " rm -f ./dist/index.d.ts ",
		"build-rollup-dts": "rollup -c rollup.dts.config.js",
		"pack": "cp ./package.json ./dist/package.json && npm pack ./dist",
		"prepublishOnly": "echo 'Did you mean to publish `./package/`, instead of `./`?' && exit 1",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@rollup/plugin-multi-entry": "^6.0.0",
		"@rollup/plugin-typescript": "^11.0.0",
		"@sveltejs/adapter-auto": "^1.0.0",
		"@sveltejs/kit": "^1.0.0",
		"@tsconfig/svelte": "^3.0.0",
		"@typescript-eslint/eslint-plugin": "^5.45.0",
		"@typescript-eslint/parser": "^5.45.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.8.1",
		"rollup-plugin-delete": "^2.0.0",
		"rollup-plugin-dts": "^5.1.1",
		"sass": "^1.57.1",
		"svelte": "^3.54.0",
		"svelte-check": "^2.9.2",
		"svelte-preprocess": "^5.0.0",
		"tslib": "^2.4.1",
		"typescript": "^4.9.4",
		"vite": "^4.0.0"
	},
	"type": "module",
	"dependencies": {
		"@rollup/plugin-node-resolve": "^15.0.1",
		"rollup": "^3.9.1",
		"rollup-plugin-svelte": "^7.1.0"
	},
	"main": "dist/cjs/index.js",
	"module": "dist/index.js",
	"typings": "dist/index.d.ts"
}
